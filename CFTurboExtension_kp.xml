<extension version="1" name="CFTurboExtension" icon="images\plm_ural.png">
    <guid shortid="CFTurbo">69f95f34-a323-4d7b-9324-00d6c9ebec27</guid>

    <script src="main_kp.py" compiled="True" />
    <script src="xml_parser.py" compiled="True" />
    <script src="tse_parser.py" compiled="True" />
    <script src="visibility.py" compiled="True" />

   <!-- <script src="test.py" compiled="True" /> !-->

    <interface context="Project">
        <images>images</images>
    </interface>
    <workflow name="wf3" context="Project" version="1">
        <tasks>

			<!-- first cell !-->

            <task name="CFTurboImport" caption="CFTurbo Import" icon="CFturbo_cell1" version="1">
                <callbacks>
					<onstatus>status</onstatus>
                    <onupdate>update</onupdate>
					<onreset>reset</onreset>
				</callbacks>
				<contextmenus>
					<entry icon="CFturbo_cell2" name="Import CFTurbo .cft-batch file..." priority="1.0" type="ContextMenuEntry" version="1">
						<callbacks>
							<onclick>edit</onclick>
						</callbacks>
					</entry>
				</contextmenus>

				<outputs>
                    <output format="" type="TurboGeometry"/>
                </outputs>

                <propertygroup name="CFTurbo batch file" caption="CFTurbo batch input file" display="caption">
                    <property name="InputFileName" caption="Input File Name" control="text" default="No file chosen!" readonly="True">
						<callbacks>
							<!--onadd>onAdd</onadd!-->
							<IsValid>InFileValid</IsValid>
						</callbacks>
                    </property>
                </propertygroup>
                <propertygroup name="MainDimensions" caption="Main dimensions of Impeller" display="caption">

					<callbacks>
							<isvisible>GroupsVisible</isvisible>
					</callbacks>


					<!-- I've commented it because it is possible to parameterized tip clearance in TurboGrid !-->
					<!--<property name="TipClearance" caption="Tip clearance" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>tipClearanceVisible</isvisible>
						</callbacks>
					</property> !-->
					<property name="HubDiameter" caption="Hub diameter" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>HubDiameterVisible</isvisible>
						</callbacks>
					</property>
					<property name="SuctionDiameter" caption="Suction diameter" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>SuctionDiameterVisible</isvisible>
						</callbacks>
					</property>
					<property name="ImpellerDiameter" caption="Impeller diameter" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>ImpellerDiameterVisible</isvisible>
						</callbacks>
					</property>
					<property name="ImpellerOutletWidth" caption="Impeller outlet width" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>ImpellerOutWidthVisible</isvisible>
						</callbacks>
					</property>

					<!-- Main dimensions of Axial Pump !-->

					<property name="HubDiameterInlet" caption="Hub diameter inlet" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>HubDiameterInletVisible</isvisible>
						</callbacks>
					</property>
					<property name="TipDiameterInlet" caption="Tip diameter inlet" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>TipDiameterInletVisible</isvisible>
						</callbacks>
					</property>
					<property name="HubDiameterOutlet" caption="Hub diameter outlet" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>HubDiameterOutVisible</isvisible>
						</callbacks>
					</property>
					<property name="TipDiameterOutlet" caption="Tip diameter outlet" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>TipDiameterOutVisible</isvisible>
						</callbacks>
					</property>

				</propertygroup>

				<!-- Blade Properties of Radial Pump !-->

				<propertygroup name="BladeProperties" caption="Blade properties" display="caption">

					<callbacks>
							<isvisible>GroupsVisible</isvisible>
					</callbacks>

					<!-- Blade thicknesses !-->

					<property name="NumberBlades" caption="Number of blades" control="integer" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="BladeThicknessLeHub" caption="Thickness leading edge at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="BladeThicknessLeShroud" caption="Thickness leading edge at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="BladeThicknessTeHub" caption="Thickness trailing edge at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="BladeThicknessTeShroud" caption="Thickness trailing edge at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>

					<!-- Blade angles !-->

					<property name="beta1h" caption="Blade angle leading edge at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>beta1Visible</isvisible>
						</callbacks>
					</property>
					<property name="beta1s" caption="Blade angle leading edge at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>beta1Visible</isvisible>
						</callbacks>
					</property>
					<property name="beta2h" caption="Blade angle trailing edge at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>beta2Visible</isvisible>
						</callbacks>
					</property>
					<property name="beta2s" caption="Blade angle trailing edge at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>beta2Visible</isvisible>
						</callbacks>
					</property>

				</propertygroup>

				<!-- wrap angles !-->

				<propertygroup name="BladeMeanLines" caption="Blade mean lines" display="caption">

					<callbacks>
							<isvisible>GroupsVisible</isvisible>
					</callbacks>

					<property name="phiLEhub" caption="Leading edge angle at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="phiLEshroud" caption="Leading edge angle at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="phiTEhub" caption="Trailing edge angle at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>
					<property name="phiTEshroud" caption="Trailing edge angle at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
						</callbacks>
					</property>

				</propertygroup>

				<!-- Meridional contour !-->

				<propertygroup name="Meridian" caption="Meridional contour" display="caption">

					<callbacks>
							<isvisible>GroupsVisible</isvisible>
					</callbacks>

					<property name="LePosHub" caption="Leading edge position at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>LePosHubVisible</isvisible>
						</callbacks>
					</property>
					<property name="LePosShroud" caption="Leading edge position at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>LePosHubVisible</isvisible>
						</callbacks>
					</property>
					<property name="TePosHub" caption="Trailing edge position at hub" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>TePosHubVisible</isvisible>
						</callbacks>
					</property>
					<property name="TePosShroud" caption="Trailing edge position at shroud" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>TePosHubVisible</isvisible>
						</callbacks>
					</property>
					<property name="LePosHubSplitter" caption="Leading edge position at hub (splitter blade)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>LePosHubSplitterVisible</isvisible>
						</callbacks>
					</property>
					<property name="LePosShroudSplitter" caption="Leading edge position at shroud (splitter blade)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>LePosHubSplitterVisible</isvisible>
						</callbacks>
					</property>

				</propertygroup>

				<!-- Blade profiles !-->

				<propertygroup name="BladeProfiles" caption="Blade profiles" display="caption">

					<callbacks>
							<isvisible>GroupsVisible</isvisible>
					</callbacks>

					<property name="BladeThickHub_1" caption="Blade thickness at hub (point 1)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickHub1Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickShroud_1" caption="Blade thickness at Shroud (point 1)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickShroud1Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickHub_2" caption="Blade thickness at hub (point 2)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickHub2Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickShroud_2" caption="Blade thickness at Shroud (point 2)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickShroud2Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickHub_3" caption="Blade thickness at hub (point 3)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickHub3Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickShroud_3" caption="Blade thickness at Shroud (point 3)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickShroud3Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickHub_4" caption="Blade thickness at hub (point 4)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickHub4Visible</isvisible>
						</callbacks>
					</property>
					<property name="BladeThickShroud_4" caption="Blade thickness at Shroud (point 4)" control="double" default="0.0" parameterizable="True">
						<callbacks>
							<isvisible>BladeThickShroud4Visible</isvisible>
						</callbacks>
					</property>

				</propertygroup>
            </task>
        </tasks>

        <taskgroups>

            <taskgroup name="CFTurbo" caption="CFTurbo Interface" icon="taskgroup" category="ACT Custom Workflows" abbreviation="CFT" version="1">
                <includeTask name="CFTurboImport" caption="CFTurbo Import"/>
            </taskgroup>

        </taskgroups>
    </workflow>
</extension>